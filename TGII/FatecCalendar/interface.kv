<Content>:
    ScrollView:
        MDList:
            OneLineIconListItem:
                text: "Aluno"     
                on_release:
                    root.nav_drawer.set_state("close")
                    root.manager.current = "aluno"
                IconLeftWidget:
                    icon: "card-account-details"

            OneLineIconListItem:
                text: "Aula"     
                on_release:
                    root.nav_drawer.set_state("close")
                    root.manager.current = "aula"
                IconLeftWidget:
                    icon: "pencil-box"

            OneLineIconListItem:
                text: "Grade"     
                on_release:
                    root.nav_drawer.set_state("close")
                    root.manager.current = "grade"
                IconLeftWidget:
                    icon: "calendar-month"
            
            OneLineIconListItem:
                text: "Faltas"
                on_release:
                    root.nav_drawer.set_state("close")
                    root.manager.current = "faltas"
                IconLeftWidget:
                    icon: "notebook-remove"

            OneLineIconListItem:
                text: "Avaliações"
                on_release:
                    root.nav_drawer.set_state("close")
                    root.manager.current = "avaliacoes"
                IconLeftWidget:
                    icon: "alert-decagram"

            OneLineIconListItem:
                text: "Notas"
                on_release:
                    root.nav_drawer.set_state("close")
                    root.manager.current = "notas"
                IconLeftWidget:
                    icon: "book-alert"
            
            OneLineIconListItem:
                text: "Extrato escolar"
                on_release:
                    root.nav_drawer.set_state("close")
                    root.manager.current = "extrato"
                IconLeftWidget:
                    icon: "school"

            OneLineIconListItem:
                text: "Sair"     
                on_release:
                    root.nav_drawer.set_state("close")
                    root.navigation("login")
                IconLeftWidget:
                    icon: "exit-to-app"
            

